<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <title>wave28 app</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <link data-turbolinks-track="true" href="~/assets/MainCss/application-b9abcf044a0bc3e705568d103eddd00e.css" media="all" rel="stylesheet" />
    <script data-turbolinks-track="true" src="~/assets/application-851b8fea8f29120d8b765082481c5168.js"></script>
    <meta content="authenticity_token" name="csrf-param" />
    <meta content="tqvG69tCeRwlAtM/I2UIBFR6LP4DHT8d1qA0giV/B+g=" name="csrf-token" />

</head>
<body id="signup">
    
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Customer Registration", "Create", "Customer")</li>
                    <li>@Html.ActionLink("Business Registration", "Register", "BusinessAdmin")</li>
                    @*<li>@Html.ActionLink("Contact", "Contact", "Home")</li>*@
                </ul>
            </div>
        </div>
    </div>

    <div class="bg clear">
        <div class="content">
            @RenderBody()
            @Scripts.Render("~/bundles/jquery")
            @Scripts.Render("~/bundles/bootstrap")
            @RenderSection("scripts", required: false)

        </div>
        <div class="bottom-wrapper">
            <div class="message">
                <span>Already have an account?</span>
                <a href="@Url.Action("Login", "Login", new {id = "loginLink"})">Sign in here</a>.
            </div>
        </div>
    </div>

</body>
</html>
